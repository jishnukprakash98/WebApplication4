@model WebApplication4.Models.Users
@{
    ViewBag.Title = "Home Page";
}
    <form action="/home" method="post">
        <div class="jumbotron">
            <h1>BA Library</h1>
            <p class="lead">BA Library is a free library for everyone in Benefitalign.</p>
            <h2>Librarion Login</h2>
            @Html.TextBoxFor(model => model.Username);
            @Html.TextBoxFor(model => model.Password);
            <input type="submit" value="Login" />
